{
  // Feed
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/v1/Post/GetAllPosts",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "host.docker.internal",
          "Port": 9000
        }
      ],
      "UpstreamPathTemplate": "/Post/GetAllPosts",
      "UpstreamHttpMethod": [ "Get" ]
    },
    // Identity
    {
      "DownstreamPathTemplate": "/api/v1/Auth/Login",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "host.docker.internal",
          "Port": 9020
        }
      ],
      "UpstreamPathTemplate": "/Auth/Login",
      "UpstreamHttpMethod": [ "Post" ]
    },
    // User
    {
      "DownstreamPathTemplate": "/api/v1/User/GetAll",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "host.docker.internal",
          "Port": 9020
        }
      ],
      "UpstreamPathTemplate": "/User/GetAll",
      "UpstreamHttpMethod": [ "Get" ]
    },
    {
      "DownstreamPathTemplate": "/api/v1/User/GetUserDetails/{userId}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "host.docker.internal",
          "Port": 9020
        }
      ],
      "UpstreamPathTemplate": "/User/GetUserDetails/{userId}",
      "UpstreamHttpMethod": [ "Get" ]
    },
    {
      "DownstreamPathTemplate": "/api/v1/User/GetUserDetailsByUserName/{userName}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "host.docker.internal",
          "Port": 9020
        }
      ],
      "UpstreamPathTemplate": "/User/GetUserDetailsByUserName/{userName}",
      "UpstreamHttpMethod": [ "Get" ]
    },
    {
      "DownstreamPathTemplate": "/api/v1/User/Create",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "host.docker.internal",
          "Port": 9020
        }
      ],
      "UpstreamPathTemplate": "/User/Create",
      "UpstreamHttpMethod": [ "Post" ]
    },
    {
      "DownstreamPathTemplate": "/api/v1/User/EditUserProfile/{id}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "host.docker.internal",
          "Port": 9020
        }
      ],
      "UpstreamPathTemplate": "/User/EditUserProfile/{id}",
      "UpstreamHttpMethod": [ "Put" ]
    },
    // Role
    {
      "DownstreamPathTemplate": "/api/v1/Role/GetAll",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "host.docker.internal",
          "Port": 9020
        }
      ],
      "UpstreamPathTemplate": "/Role/GetAll",
      "UpstreamHttpMethod": [ "Get" ]
    },
    {
      "DownstreamPathTemplate": "/api/v1/Role/Create",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "host.docker.internal",
          "Port": 9020
        }
      ],
      "UpstreamPathTemplate": "/Role/Create",
      "UpstreamHttpMethod": [ "Post" ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:9010"
  }
}